{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabledManagers": [
    "poetry",
    "npm"
  ],
  "includePaths": [
    "pyproject.toml",
    "frontend/package.json"
  ],
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "matchPaths": [
        "frontend/package.json"
      ],
      "prCreation": "never",
      "groupName": "Frontend Dependencies"
    }
  ],
  "dependencyDashboard": true
}
